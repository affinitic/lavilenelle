<?xml version="1.0"?>
<object name="portal_skins"
        allow_any="False"
        cookie_persistence="False"
        default_skin="La chorale La Vilenelle Site">

    <object name="lavilenelle_skin_images"
            meta_type="Filesystem Directory View"
            directory="lavilenelle.skin:skins/lavilenelle_skin_images"/>
    
     <object name="lavilenelle_skin_styles"
             meta_type="Filesystem Directory View"
             directory="lavilenelle.skin:skins/lavilenelle_skin_styles"/>


    <skin-path name="La chorale La Vilenelle Site"
               based-on="Sunburst Theme">
        <layer name="lavilenelle_skin_images"
               insert-after="custom"/>
          <layer name="lavilenelle_skin_styles"
                 insert-after="lavilenelle_skin_images"/>


    </skin-path>

</object>
